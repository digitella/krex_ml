\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <h2 class=\"mbr-section-title pb-3 mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Competencies</h2>\n\n        <div class=\"row\">\n            <div class=\"col-md-4\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">UI and UX</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n            <div class=\"col-md-4\" mbr-if=\"cardsAmount > 1\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Web</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n            <div class=\"col-md-4\" mbr-if=\"cardsAmount > 2\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Hybrid Apps</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n            <div class=\"col-md-4\" mbr-if=\"cardsAmount > 3\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Design</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n            <div class=\"col-md-4\" mbr-if=\"cardsAmount > 4\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Convergence</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n            <div class=\"col-md-4\" mbr-if=\"cardsAmount > 5\">\n                <label class=\"container2\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Automation</p>\n                    <input type=\"checkbox\" checked>\n                    <span class=\"checkmark\"></span>\n                </label>\n            </div>\n\n        </div>\n    </div>\n</section>",
